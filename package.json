{
  "name": "express-template",
  "version": "0.1.0",
  "description": "express template",
  "author": "CaoMeiYouRen",
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.js",
    "build": "rimraf dist && tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "rm": "rimraf node_modules",
    "watch": "tsc -w",
    "start": "node ./dist/index.js",
    "test": "npm run test:e2e",
    "test:unit": "mocha -r ts-node/register test/register.ts src/**/*.test.ts -R nyan -t 10000",
    "test:e2e": "mocha -r ts-node/register test/register.ts test/**/*.test.ts -R nyan -t 10000"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-formidable": "^1.0.5",
    "@types/express-rate-limit": "^6.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/http-proxy": "^1.17.9",
    "@types/mocha": "^9.1.1",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.14.32",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "colors": "^1.4.0",
    "cross-env": "^7.0.2",
    "debug": "^4.3.4",
    "eslint": "^8.19.0",
    "eslint-config-cmyr": "^1.1.20",
    "mocha": "^10.0.0",
    "rimraf": "^3.0.2",
    "should": "^13.2.3",
    "supertest": "^6.2.4",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-formidable": "^1.2.0",
    "express-rate-limit": "^6.4.0",
    "file-stream-rotator": "^0.6.1",
    "fs-extra": "^10.1.0",
    "http-proxy": "^1.18.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0"
  }

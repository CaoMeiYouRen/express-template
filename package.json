{
  "name": "express-template",
  "version": "0.1.0",
  "description": "express template",
  "author": "CaoMeiYouRen",
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.js",
    "build": "rimraf dist && tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "rm": "rimraf node_modules",
    "watch": "tsc -w",
    "start": "node ./dist/index.js",
    "test": "npm run test:e2e",
    "test:unit": "mocha -r ts-node/register test/register.ts src/**/*.test.ts -R nyan -t 10000",
    "test:e2e": "mocha -r ts-node/register test/register.ts test/**/*.test.ts -R nyan -t 10000"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.1",
    "@types/express-formidable": "^1.0.5",
    "@types/express-rate-limit": "^6.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/http-proxy": "^1.17.16",
    "@types/mocha": "^10.0.10",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "colors": "^1.4.0",
    "cross-env": "^10.0.0",
    "debug": "^4.4.1",
    "eslint": "^8.57.1",
    "eslint-config-cmyr": "^1.1.33",
    "mocha": "^11.7.1",
    "rimraf": "^6.0.1",
    "should": "^13.2.3",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.14",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-formidable": "^1.2.0",
    "express-rate-limit": "^8.0.1",
    "file-stream-rotator": "^1.0.0",
    "fs-extra": "^11.3.1",
